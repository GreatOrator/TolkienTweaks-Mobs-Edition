{
  "multipart": [
    {
      "apply": {
        "model": "tolkienmobs:block/pane_ammolite_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "tolkienmobs:block/pane_ammolite_side"
      }
    },
    {
      "when": {
        "north": "false"
      },
      "apply": {
        "model": "tolkienmobs:block/pane_ammolite_noside"
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "tolkienmobs:block/pane_ammolite_side_alt"
      }
    },
    {
      "when": {
        "south": "false"
      },
      "apply": {
        "model": "tolkienmobs:block/pane_ammolite_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "tolkienmobs:block/pane_ammolite_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": "false"
      },
      "apply": {
        "model": "tolkienmobs:block/pane_ammolite_noside",
        "y": 270
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "tolkienmobs:block/pane_ammolite_side",
        "y": 90
      }
    },
    {
      "when": {
        "east": "false"
      },
      "apply": {
        "model": "tolkienmobs:block/pane_ammolite_noside_alt"
      }
    }
  ]
}